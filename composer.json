{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.17",
    "fakerphp/faker": "^1.23",
    "gesdinet/jwt-refresh-token-bundle": "^1.2",
    "jawira/case-converter": "^3.5",
    "league/flysystem-aws-s3-v3": "*",
    "league/flysystem-bundle": "^3.3",
    "lexik/jwt-authentication-bundle": "^2.20",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.24",
    "predis/predis": "^2.2",
    "symfony/amqp-messenger": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/doctrine-bridge": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/event-dispatcher": "6.4.*",
    "symfony/flex": "^2",
    "symfony/form": "6.4.*",
    "symfony/framework-bundle": "6.4.*",
    "symfony/lock": "6.4.*",
    "symfony/mailer": "6.4.*",
    "symfony/mime": "6.4.*",
    "symfony/notifier": "6.4.*",
    "symfony/property-access": "6.4.*",
    "symfony/property-info": "6.4.*",
    "symfony/runtime": "6.4.*",
    "symfony/security-bundle": "6.4.*",
    "symfony/serializer": "6.4.*",
    "symfony/validator": "6.4.*",
    "symfony/yaml": "6.4.*"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.0",
    "doctrine/doctrine-fixtures-bundle": "^3.5",
    "friendsofphp/php-cs-fixer": "^3.40",
    "phpunit/phpunit": "^10.4",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/maker-bundle": "^1.51",
    "symfony/phpunit-bridge": "^7.0",
    "vimeo/psalm": "^5.16"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "psalm": [
      "php ./vendor/bin/psalm --show-info=true"
    ],
    "php-cs-fixer": [
      "php ./vendor/bin/php-cs-fixer --config=.php-cs-fixer.dist.php --quiet fix src",
      "php ./vendor/bin/php-cs-fixer --config=.php-cs-fixer.dist.php --quiet fix tests"
    ],
    "fix-code": [
      "@php-cs-fixer",
      "@psalm"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*"
    }
  }
}
